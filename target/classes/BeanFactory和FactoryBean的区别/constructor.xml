<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd" profile="dev">

    <bean id="student" class="BeanFactory和FactoryBean的区别.StudentFactoryBean" p:studentInfo="张三,25,三年二班"/>

    <bean id="furniture" class="BeanFactory和FactoryBean的区别.FurnitureFactoryBean" p:furniture="desk"/>
    <bean id="furniture1" class="BeanFactory和FactoryBean的区别.FurnitureFactoryBean" p:furniture="chair"/>
</beans>